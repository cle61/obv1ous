<div class="row">
	<div class="col s12">
		<%= form_for(@member, url: admin_team_members_path, html: { method: :post }, id: 'new_news') do |f| %>

			<% @member.errors.full_messages.each do |msg| %>
				<%= msg %>
			<% end %>

			<div class="field input-field">
				<%= f.label :name, "Nom" %>
				<%= f.text_field :name, autofocus: true, :class => "validate" %>
			</div>

			<div class="field input-field">
				<%= f.label :id_steam, "ID Steam" %>
				<%= f.text_field :id_steam, :class => "validate" %>
			</div>
			
			<div class="btnlog actions">
				<%= button_tag(type: 'submit', class: "btn") do %>
					Publier <i class='material-icons right'>send</i>
				<% end %>
			</div>
		<% end %>

		<%= link_to 'Retour', admin_team_members_path %>
	</div>
</div>
